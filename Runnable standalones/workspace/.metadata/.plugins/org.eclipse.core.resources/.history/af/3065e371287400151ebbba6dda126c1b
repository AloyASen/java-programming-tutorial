package File_readers;

public class Word_details_enlister {
	String wordS;
	int Freq;
	Word_details_enlister np;
	public int getFreq() {
		return Freq;
	}
	public void setFreq(int freq) {
		Freq = freq;
	}
	public Word_details_enlister getNp() {
		return np;
	}
	public void setNp(Word_details_enlister np) {
		this.np = np;
	}
	public Word_details_enlister(String wrd , int fq) {
		this.wordS=wrd;
		this.Freq=fq;
		np=null;
	}
	
}
